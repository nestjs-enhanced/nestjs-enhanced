{
  "name": "@nestjs-enhanced/sockets",
  "version": "1.2.3",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "preversion": "echo \"//registry.npmjs.org/:_authToken=${NODE_TOKEN}\" >> ./.npmrc",
    "build": "npm run clean && npm run compile && npx gen-esm-wrapper dist/index.js dist/index.mjs",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.build.json"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js"
    }
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "socket.io": "^4.7.2"
  },
  "dependencies": {
    "@nestjs-enhanced/core": "*",
    "@nestjs-enhanced/pub-sub": "*",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs-enhanced/core": "*",
    "@nestjs-enhanced/pub-sub": "*",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/websockets": "^10.2.7",
    "@types/express": "^4.17.20",
    "@types/uuid": "^9.0.6",
    "socket.io": "^4.7.2"
  }
}
